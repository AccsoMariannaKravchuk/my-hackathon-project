{
  "name": "my-hackathon-project",
  "version": "1.0.0",
  "description": "Minimal Express API for hackathon development",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "save": "git add . && git commit -m \"Auto-save checkpoint: $(date)\" && git push && echo \"ðŸ“„ Checkpoint created: $(date)\" > checkpoint_$(date +%Y%m%d_%H%M%S).txt && echo \"âœ… All changes saved, committed, pushed, and checkpoint created!\""
  },
  "keywords": [
    "express",
    "api",
    "hackathon"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
